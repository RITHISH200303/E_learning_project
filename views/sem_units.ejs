<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <%- include('includes/head') %>
    <link rel="stylesheet" href="/styles/sem_units.css" />
  </head>
  <body>
    <%- include('includes/navebar') %>
    <div class="units_containor">
      <% for(const subjectUnit in subjectUnits) { %>

      <a
        href="<%= subjectUnits[subjectUnit] %>/<%= subjectDepartment %>/<%= subjectName %>  "
        class="unit_buttons"
        ><%= subjectUnits[subjectUnit]%>
      </a>
      <% } %>
    </div>
    <div class="quizContainor">
      <a class="quiz" href="/quiz/<%= subjectDepartment %>/<%= subjectName %> "
        ><span class="show">Quiz</span></a
      >
    </div>
  </body>
</html>
